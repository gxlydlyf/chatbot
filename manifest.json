{
  "name": "ChatGPT FREE",
  "description": "ChatGPT FREE 的PWA版本",
  "version": "1.7",
  "short_name": "ChatGPT",
  "icons": [
    {
      "src": "icon/windows11/SplashScreen.scale-400.png",
      "sizes": "2480x1200"
    },
    {
      "src": "icon/windows11/Square44x44Logo.altform-lightunplated_targetsize-256.png",
      "sizes": "256x256"
    },
    {
      "src": "icon/ios/144.png",
      "sizes": "144x144"
    },
    {
      "src": "icon/android/android-launchericon-512-512.png",
      "sizes": "512x512"
    }
  ],
  "permissions": [
    "tabs",
    "storage",
    "activeTab",
    "http://*/*",
    "cookies"
  ],
  "protocol_handlers": [
    {
      "protocol": "web+chatgpt",
      "url": "/index.html?page=%s"
    }
  ],
  "browser_action": {
    "default_icon": "img/icons8-setting.png",
    "default_title": "设置",
    "default_popup": "setting.html"
  },
  "screenshots": [
    {
      "src": "screenshots/narrow.png",
      "sizes": "469x834",
      "type": "image/png",
      "form_factor": "narrow",
      "label": "很棒的应用程序的主屏幕"
    },
    {
      "src": "screenshots/wide.png",
      "sizes": "1600x1000",
      "type": "image/png",
      "form_factor": "wide",
      "label": "很棒的应用程序的主屏幕"
    }
  ],
  "start_url": "/index.html",
  "id": "/index.html",
  "display": "standalone",
  "manifest_version": 2,
  "background_color": "white",
  "theme_color": "#ffffff",
  "display_override": [
    "window-controls-overlay"
  ],
  "prefer_related_applications": false,
  "dir": "ltr",
  "lang": "zh-CN"
}
